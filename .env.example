# AWS基本設定
AWS_REGION=ap-northeast-1
AWS_ACCOUNT_ID=

# Knowledge Base設定（既存のKnowledge Base IDを指定）
KNOWLEDGE_BASE_ID=

# フロー設定
FLOW_NAME=test-log-analysis-flow

# IAMロール設定
EXECUTION_ROLE_NAME=BedrockFlowExecutionRole
EXECUTION_ROLE_POLICY_NAME=BedrockFlowExecutionPolicy

# Bedrockモデル設定（各ノードで異なるモデルを使用可能）
# 東京リージョンではapac.プレフィックスのinference profileを使用
MODEL_ID_ANALYZER=apac.anthropic.claude-sonnet-4-20250514-v1:0
MODEL_ID_EXTRACTOR=apac.anthropic.claude-3-haiku-20240307-v1:0
MODEL_ID_QUERY_GENERATOR=apac.anthropic.claude-3-haiku-20240307-v1:0
MODEL_ID_REPORT=apac.anthropic.claude-3-5-sonnet-20241022-v2:0
MODEL_ID_KNOWLEDGE_BASE=apac.anthropic.claude-3-7-sonnet-20250219-v1:0

# トークン数設定
MAX_TOKENS_ANALYZER=3000
MAX_TOKENS_EXTRACTOR=2000
MAX_TOKENS_REPORT=4000

# モデルパラメータ
TEMPERATURE=0
TOP_P=1
