# AWS基本設定
AWS_REGION=ap-northeast-1
AWS_ACCOUNT_ID=

# Knowledge Base設定（既存のKnowledge Base IDを指定）
KNOWLEDGE_BASE_ID=

# フロー設定
FLOW_NAME=test-log-analysis-flow

# IAMロール設定
EXECUTION_ROLE_NAME=BedrockFlowExecutionRole
EXECUTION_ROLE_POLICY_NAME=BedrockFlowExecutionPolicy

# Bedrockモデル設定（各ノードで異なるモデルを使用可能）
# 全てのモデルでinference profile形式（us.プレフィックス）を使用
MODEL_ID_ANALYZER=us.anthropic.claude-sonnet-4-20250514-v1:0
MODEL_ID_EXTRACTOR=us.amazon.nova-pro-v1:0
MODEL_ID_QUERY_GENERATOR=us.anthropic.claude-3-haiku-20240307-v1:0
MODEL_ID_REPORT=us.amazon.nova-micro-v1:0
MODEL_ID_KNOWLEDGE_BASE=us.anthropic.claude-3-7-sonnet-20250219-v1:0

# トークン数設定
MAX_TOKENS_ANALYZER=3000
MAX_TOKENS_EXTRACTOR=2000
MAX_TOKENS_REPORT=4000

# モデルパラメータ
TEMPERATURE=0
TOP_P=1
